<h1> Login </h1>
<%= link_to "Login with Tumblr", '/auth/tumblr', method: :post%>
<%= form_with(url: '/login')do |f|%>
        <%= f.label "Username:" %>
        <%= f.text_field :username %><br><br>
        <%= f.label :password %>
        <%= f.password_field :password %><br><br>
    
        <%= f.submit "Log In"%>
    <% end %>